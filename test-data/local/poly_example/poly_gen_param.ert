JOBNAME poly_%d

QUEUE_SYSTEM LOCAL
QUEUE_OPTION LOCAL MAX_RUNNING 50

RUNPATH poly_out/real_%d/iter_%d

OBS_CONFIG observations
TIME_MAP time_map

NUM_REALIZATIONS 10
MIN_REALIZATIONS 1

GEN_PARAM COEFFS_PARAM coeffs.txt INPUT_FORMAT:ASCII OUTPUT_FORMAT:ASCII INIT_FILES:init_files/init_%d
GEN_DATA POLY_RES RESULT_FILE:poly_%d.out REPORT_STEPS:0 INPUT_FORMAT:ASCII

INSTALL_JOB poly_eval POLY_EVAL
INSTALL_JOB param2kw PARAM2KW

SIMULATION_JOB param2kw coeffs.txt coeffs.json
SIMULATION_JOB poly_eval

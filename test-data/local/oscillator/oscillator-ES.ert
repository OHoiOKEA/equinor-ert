JOBNAME oscillator_%d

RANDOM_SEED 12345678

ANALYSIS_SELECT STD_ENKF
ANALYSIS_SET_VAR STD_ENKF IES_INVERSION 0

QUEUE_SYSTEM LOCAL
QUEUE_OPTION LOCAL MAX_RUNNING 50

RUNPATH oscillator_out/realization-%d/iter-%d

OBS_CONFIG observations
TIME_MAP time_map

NUM_REALIZATIONS 100
MIN_REALIZATIONS 1

GEN_KW COEFFS coeff.tmpl coeffs.json coeff_priors
GEN_DATA OSCILLATOR_RES RESULT_FILE:oscillator_%d.out REPORT_STEPS:0 INPUT_FORMAT:ASCII

INSTALL_JOB oscillator_eval OSCILLATOR_EVAL
SIMULATION_JOB oscillator_eval

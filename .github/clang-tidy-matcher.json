{
  "problemMatcher": [
    {
      "owner": "clang-tidy-error",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^([^:]*):(\\d+):(\\d+): error: (.*?)\\[([^\\]]+\\])$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4,
          "code": 5
        }
      ]
    },
    {
      "owner": "clang-tidy-warning",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^([^:]*):(\\d+):(\\d+): warning: (.*?)\\[([^\\]]+\\])$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4,
          "code": 5
        }
      ]
    }
  ]
}
